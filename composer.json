{
  "name": "greg0/string-builder",
  "description": "Simple PHP string builder inspired by C# StringBuilder",
  "keywords": [
    "php",
    "simple",
    "string",
    "builder"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Greg0/string-builder",
  "authors": [
    {
      "name": "Grzegorz KuÅºnik",
      "email": "gerg0sz92@gmail.com",
      "role": "Owner",
      "homepage": "https://github.com/Greg0"
    }
  ],
  "support": {
    "issues": "https://github.com/Greg0/string-builder/issues",
    "source": "https://github.com/Greg0/string-builder"
  },
  "type": "library",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3"
  },
  "suggest": {
    "ext-mbstring": "Needed mb extension support. You can also install symfony/polyfill-mbstring"
  },
  "autoload": {
    "psr-4": {
      "Greg0\\StringBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Greg0\\StringBuilder\\": "tests/"
    }
  },
  "scripts": {
    "tests": "./vendor/bin/phpunit"
  }
}
